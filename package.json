{
  "name": "messaging-system-personalization-experiment-1-addon",
  "version": "1.0.0",
  "description": "messaging-system-personalization-experiment-1-addon",
  "private": true,
  "license": "MPLv2",
  "repository": {
    "type": "git",
    "url": "git://github.com/mozilla/messaging-system-personalization-experiment-1-addon.git"
  },
  "scripts": {
    "start:control": "npm run build && web-ext run -s dist/extension-control-$npm_package_version",
    "start:treatment": "npm run build && web-ext run -s dist/extension-treatment-$npm_package_version",
    "prebuild": "ajv compile -s schemas/messaging-system-personalization-experiment-1-update.payload.schema.json -o /tmp/validate.js && (cat /tmp/validate.js | grep -v 'module.exports = validate;' > src/validate.js)",
    "build": "node build.js",
    "clean": "rimraf dist web-ext-artifacts",
    "lint": "eslint . --ext jsm,js,json",
    "format": "npm-run-all -s -n format:prettier format:eslint",
    "format:eslint": "eslint . --ext jsm,js,json --fix",
    "format:prettier": "prettier '**/*.{css,js,json,jsm,md}' --trailing-comma=all --ignore-path=.eslintignore --write",
    "generate": "npm-run-all -s -n generate:generateSchema:* generate:verifyWeeSchema:* generate:documentSchema:* generate:generateStubApi:* format",
    "generate:generateSchema:addonsMetadata": "cd src/privileged/addonsMetadata && yaml2json schema.yaml -p > schema.json",
    "generate:generateSchema:clientContext": "cd src/privileged/clientContext && yaml2json schema.yaml -p > schema.json",
    "generate:generateSchema:messagingSystem": "cd src/privileged/messagingSystem && yaml2json schema.yaml -p > schema.json",
    "generate:generateSchema:privacyContext": "cd src/privileged/privacyContext && yaml2json schema.yaml -p > schema.json",
    "generate:generateSchema:personalizedCfrPrefs": "cd src/privileged/personalizedCfrPrefs && yaml2json schema.yaml -p > schema.json",
    "generate:generateSchema:remoteSettings": "cd src/privileged/remoteSettings && yaml2json schema.yaml -p > schema.json",
    "generate:generateSchema:testingOverrides": "cd src/privileged/testingOverrides && yaml2json schema.yaml -p > schema.json",
    "generate:documentSchema:addonsMetadata": "cd src/privileged/addonsMetadata && documentSchema schema.json > api.md",
    "generate:documentSchema:clientContext": "cd src/privileged/clientContext && documentSchema schema.json > api.md",
    "generate:documentSchema:messagingSystem": "cd src/privileged/messagingSystem && documentSchema schema.json > api.md",
    "generate:documentSchema:privacyContext": "cd src/privileged/privacyContext && documentSchema schema.json > api.md",
    "generate:documentSchema:personalizedCfrPrefs": "cd src/privileged/personalizedCfrPrefs && documentSchema schema.json > api.md",
    "generate:documentSchema:remoteSettings": "cd src/privileged/remoteSettings && documentSchema schema.json > api.md",
    "generate:documentSchema:testingOverrides": "cd src/privileged/testingOverrides && documentSchema schema.json > api.md",
    "generate:generateStubApi:addonsMetadata": "# cd src/privileged/addonsMetadata && generateStubApi ./schema.json > stubApi.js",
    "generate:generateStubApi:clientContext": "# cd src/privileged/clientContext && generateStubApi ./schema.json > stubApi.js",
    "generate:generateStubApi:messagingSystem": "# cd src/privileged/messagingSystem && generateStubApi ./schema.json > stubApi.js",
    "generate:generateStubApi:privacyContext": "# cd src/privileged/privacyContext && generateStubApi ./schema.json > stubApi.js",
    "generate:generateStubApi:personalizedCfrPrefs": "# cd src/privileged/personalizedCfrPrefs && generateStubApi ./schema.json > stubApi.js",
    "generate:generateStubApi:remoteSettings": "# cd src/privileged/remoteSettings && generateStubApi ./schema.json > stubApi.js",
    "generate:generateStubApi:testingOverrides": "# cd src/privileged/testingOverrides && generateStubApi ./schema.json > stubApi.js",
    "generate:verifyWeeSchema:addonsMetadata": "cd src/privileged/addonsMetadata && verifyWeeSchema schema.json",
    "generate:verifyWeeSchema:clientContext": "cd src/privileged/clientContext && verifyWeeSchema schema.json",
    "generate:verifyWeeSchema:messagingSystem": "cd src/privileged/messagingSystem && verifyWeeSchema schema.json",
    "generate:verifyWeeSchema:privacyContext": "cd src/privileged/privacyContext && verifyWeeSchema schema.json",
    "generate:verifyWeeSchema:personalizedCfrPrefs": "cd src/privileged/personalizedCfrPrefs && verifyWeeSchema schema.json",
    "generate:verifyWeeSchema:remoteSettings": "cd src/privileged/remoteSettings && verifyWeeSchema schema.json",
    "generate:verifyWeeSchema:testingOverrides": "cd src/privileged/testingOverrides && verifyWeeSchema schema.json"
  },
  "devDependencies": {
    "ajv-cli": "^3.0.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-mozilla": "^0.4.4",
    "eslint-plugin-no-unsanitized": "^2.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "klaw": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.3",
    "safe-es6-template": "^1.0.1",
    "web-ext": "^2.6.0",
    "webext-experiment-utils": "github:mozilla/webext-experiment-utils#v0.2.0",
    "yamljs": "^0.3.0"
  },
  "dependencies": {},
  "resolutions": {
    "lodash": ">=4.17.11",
    "https-proxy-agent": "^2.2.3",
    "set-value": ">=2.0.1 <3.0.0 || >=3.0.1",
    "mixin-deep": ">=1.3.2 <2.0.0 || >=2.0.1"
  }
}
